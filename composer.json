{
    "name": "shmurakami/api-doc-generator",
    "description": "generate swagger yaml/json or doc comment for apidocjs from API response json",
    "license": "MIT",
    "authors": [
        {
            "name": "Shaka",
            "email": "shmurakami1104@gmail.com"
        }
    ],
    "require": {
        "symfony/yaml": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0.0"
    },
    "autoload": {
      "psr-4": {
        "shmurakami\\ApiDocGenerator\\": "src/ApiDocGenerator/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "shmurakami\\ApiDocGenerator\\Tests\\": "tests/ApiDocGenerator/"
      }
    },
    "bin": [
      "bin/parse"
    ]
}
